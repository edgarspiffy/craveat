<%- include("../partials/header")  %>
<div class="container">
  <a href="/spot" class="btn btn-primary">Go Back</a>
</div>

<div class="container">
  <h1>Your Food Spot Info</h1>
  <form action='/spot' method='POST'>
    <div class="form-group">
      <label for="restaurantName">Name</label>
      <input type="text" class="form-control" name='spot[name]' id="restaurantName" placeholder="">
    </div>

    <div class="form-group">
      <label for="restaurantCity">City</label>
      <input type="text" class="form-control" name='spot[city]' id="restaurantCity" placeholder="">
    </div>


    <div>
      <button class='btn btn-primary'>Submit</button>
    </div>
  </form>
</div>



<%- include("../partials/footer")  %>