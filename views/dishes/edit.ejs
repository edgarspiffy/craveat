<%- include("../partials/header")  %>


<div class="container">
  <h1>Edit Dish</h1>
  <form action='/spot/<%= id %>/dishes/<%= dish._id %>?_method=PUT' method='POST'>
    <div class="form-group">
      <label for="restaurantName">NAME</label>
      <input type="text" class="form-control" name='dish[name]' id="restaurantName" value="<%= dish.name %> ">
    </div>

    <div class="form-group">
      <label for="restaurantCity">INGREDIENTS</label>
      <input type="text" class="form-control" name='dish[ingredients]' id="restaurantCity" value="<%= dish.ingredients %> ">
    </div>


    <div>
      <button class='btn btn-primary'>Submit</button>
    </div>
  </form>
</div>



<%- include("../partials/footer")  %>